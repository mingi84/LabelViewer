    //---------------------Draw Template---------------------------------

    function drawCanvas(data, i) {
        const canvasindex = "canvas" + i;
        const canvas = document.getElementById(canvasindex);
        const ctx = canvas.getContext('2d');

        // Draw keypoints
        data.keypoints.forEach(point => {
            ctx.beginPath();
            ctx.arc(point.x, point.y, 5, 0, 2 * Math.PI);
            ctx.fillStyle = 'blue';
            ctx.fill();
            ctx.closePath();

            // Draw the text with the key value over the point
            ctx.font = '12px Arial';
            ctx.fillStyle = 'black';
            ctx.fillText('TEXTO', point.x + 8, point.y + 4);
        });

        // Draw lines
        data.lines.forEach(line => {
            ctx.beginPath();
            ctx.moveTo(line.from.x, line.from.y);
            ctx.lineTo(line.to.x, line.to.y);
            ctx.strokeStyle = 'red';
            ctx.lineWidth = 2;
            ctx.stroke();
            ctx.closePath();
        });
    }

    function resizeCanvasToDisplaySize(canvas) {
        // 브라우저가 캔버스를 표시하고 있는 크기를 CSS 픽셀 단위로 얻어옵니다.
        const dpr = window.devicePixelRatio;
        const displayWidth = Math.round(canvas.clientWidth * dpr);
        const displayHeight = Math.round(canvas.clientHeight * dpr);

        // 캔버스와 크기가 다른지 확인합니다.
        const needResize = canvas.width !== displayWidth ||
            canvas.height !== displayHeight;

        if (needResize) {
            // 캔버스를 동일한 크기가 되도록 합니다.
            canvas.width = displayWidth;
            canvas.height = displayHeight;
        }
        return needResize;
    }

    //Light box
    var originwidth = "";
    var originheight = "";

    function lightboxDrawTemplate(canvasIndex) {
        const canvas = document.getElementById("zoomcanv" + canvasIndex);
        const ctx = canvas.getContext('2d');

        if (originwidth == "" && originheight == "") {
            originwidth = canvas.width;
            originheight = canvas.height;
        }

        resizeCanvasToDisplaySize(canvas);
        z_width = (canvas.width / originwidth); //배율 구하기 x
        z_height = (canvas.height / originheight); // 배율 구하기 y

        zoomx = canvas.width - originwidth;
        zoomy = canvas.height - originheight;

        var selectedID = [];
        $("input[name='tag-label-lightbox']:checked").each(function () {
            var tmpVal = $(this).val();
            selectedID.push(tmpVal);
        });

        ctx.clearRect(0, 0, canvas.width, canvas.height);

        for (var j = 0; j < selectedID.length; j++) {
            /*선택한 템플릿에 맞는 키포인트를 그린다.*/
            templateKeyMatch.forEach((list, ind) => {
                if (list.keypoint_id == selectedID[j]) {
                    if (list.keypoint_list[j].keypoints.length > 0) {

                        list.keypoint_list[canvasIndex - 1].keypoints.forEach(point => {
                            pointcolor = point.key;
                            ctx.beginPath();
                            ctx.arc(point.x * z_width, point.y * z_height, 1 * z_width, 0, 2 * Math.PI);
                            ctx.fillStyle = pointcolor;
                            ctx.fill();
                            ctx.strokeStyle = pointcolor;
                            ctx.lineWidth = 0.5 * z_width;
                            ctx.stroke();
                            ctx.closePath();
                        });
                    }
                }

            });

            // Draw lines
            //console.log(linklines);
            for (i = 0; i < linklines.length; i++) {
                linklines[i].lines.forEach((line, ind) => {
                    if (linklines[i].keypoint_id == selectedID[j]) {
                        if (templateKeyMatch[i].keypoint_list[j].keypoints.length > 0) {

                            var linecolor = linklines[i].linescolor;
                            const from = templateKeyMatch[i].keypoint_list[canvasIndex - 1].keypoints[line[0]];
                            const to = templateKeyMatch[i].keypoint_list[canvasIndex - 1].keypoints[line[1]];

                            ctx.beginPath();
                            ctx.moveTo(from.x * z_width, from.y * z_height);
                            ctx.lineTo(to.x * z_width, to.y * z_height);
                            ctx.strokeStyle = linecolor;
                            ctx.lineWidth = 1 * z_width;
                            ctx.stroke();
                            ctx.closePath();
                        }
                    }
                });
            }
        }


        for (var j = 0; j < selectedID.length; j++) {
            //Draw lect

            for (var i = 0; i < templateKeyMatch.length; i++) {
                ctx.scale(1 * z_width * 0.645, 1 * z_height * 0.568)
                if (templateKeyMatch[i].keypoint_id == selectedID[j]) {
                    ctx.lineWidth = 2;
                    ctx.strokeStyle = templateKeyMatch[i].keypoint_list[canvasIndex - 1].bbox_color;
                    ctx.strokeRect(templateKeyMatch[i].keypoint_list[canvasIndex - 1].bbox[0],
                        templateKeyMatch[i].keypoint_list[canvasIndex - 1].bbox[1],
                        templateKeyMatch[i].keypoint_list[canvasIndex - 1].bbox[2],
                        templateKeyMatch[i].keypoint_list[canvasIndex - 1].bbox[3]);

                }
                ctx.scale(1 / z_width / 0.645, 1 / z_height / 0.568)

            }


        }
    }

