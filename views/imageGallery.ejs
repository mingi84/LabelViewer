    function countPage(attr) {
        xylocationlist = [];
        linklines = [];
        var pageCnt = Number(document.getElementById("page-count").value);
        var pageMax = Number(document.getElementById("page-total").innerText);

        if (attr == "up") {
            if (pageCnt < 1 || pageCnt >= pageMax) {
                return;
            }
            document.getElementById("page-count").value = Number(pageCnt) + 1;
    
            getDatasetInfo(Number(pageCnt) + 1);

        } else if (attr == "down") {
            if (pageCnt <= 1 || pageCnt > pageMax) {
                return;
            }

            document.getElementById("page-count").value = Number(pageCnt) - 1;

            getDatasetInfo(Number(pageCnt) - 1);

        } else {
            if (attr < 1) {
                attr = 1;
            }
            if (attr > pageMax) {
                attr = pageMax;
            }
            document.getElementById("page-count").value = attr;
            getDatasetInfo(Number(attr));

        }
    }






