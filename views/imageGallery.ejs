    function XvaluesBeforeAndAfterText(arr, text) {
        const xvaluesBefore = [];
        const xvaluesAfter = [];
        let indexFound = false;

        for (let i = 0; i < arr.length; i++) {
            if (arr[i] === text) {
                indexFound = true;
                continue;
            }
            if (indexFound) {
                xvaluesAfter.push(arr[i]);
            } else {
                xvaluesBefore.push(arr[i]);
            }
        }

        if (!indexFound) {
            return {
                xvaluesBefore: [],
                xvaluesAfter: []
            };
        }
        return {
            xvaluesBefore,
            xvaluesAfter
        };
    }

